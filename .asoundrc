# $Id: $HOME/.asoundrc,v 1.1 2013/03/16 -tclover Exp $
#
## HW
pcm.hda { type hw; card Intel; device 1 }
pcm.cmi { type hw; card CMI8788; device 1 }
## DEFAULT
pcm.!default { type plug; slave.pcm jackplug }
ctl.!default { type hw; card Intel }
## BS2B
ctl.bs2b { type hw; card Intel }
pcm.bs2b { type plug; slave.pcm plugbs2b }
pcm.plugbs2b {
	type ladspa
	slave.pcm jackplug
	path /usr/lib/ladspa
	plugins [
	{
		id 4221
		label bs2b
		input { controls [ 700 4.5 ] }
	}
	]
}
## JACK output
ctl.jack { type hw; card Intel }
pcm.jack { type plug; slave.pcm jackplug }
pcm.jackplug {
	type jack
	playback_ports 
	{
		0 alsa_pcm:playback_1
		1 alsa_pcm:playback_2
	}
	capture_ports 
	{
		0 alsa_pcm:capture_1
		1 alsa_pcm:capture_2
	}
}
## MBEQ
ctl.eq { type hw; card Intel }
pcm.eq { type plug; slave.pcm plugeq }
pcm.plugeq {
	type ladspa
	slave.pcm jackplug
	path /usr/lib/ladspa
	plugins [
	{
		label mbeq
		id 1197
		input {
			#bands: 50hz,100hz,156hz,220hz,311hz,440hz,622hz,880hz,
			#1250hz,1750hz,25000hz,50000hz,10000hz,20000hz
			controls [ -5 -5 -5 -5 -5 -10 -20 -15 -10 -10 -10 -10 -10 -3 -2 ]
		}
	}
	]
}
## EQUALIZER
ctl.equal { type equal }
pcm.equal { type plug; slave.pcm plugequal }
pcm.plugequal { type equal; slave.pcm jackplug }
## ALSA-EMU
#pcm.alsa {
#	type oss
#	device /dev/oss/oss_cmi878x0/pcm1
#}
#mixer.alsa {
#	type oss
#	device /dev/oss/oss_cmi878x0/mix1
#}
## OSS-EMU
ctl.oss { type oss }
pcm.oss { type plug; slave.pcm jackplug }
