# $Id: $HOME/.asoundrc, 2014/07/22 11:47:33 -tclover Exp $
#
#-*- HW -*-
pcm.hdi { type hw; card HDMI }
pcm.pch { type hw; card PCH }
pcm.cmi { type hw; card CMI8788; device 1 }
#-*- DEFAULT -*-
pcm.!default { type plug; slave.pcm dmixer }
ctl.!default { type hw; card PCH }
#-*- BS2B -*-
ctl.bs2b { type plug; slave.ctl default }
pcm.bs2b { type plug; slave.pcm plugbs2b }
pcm.plugbs2b {
	type ladspa
	slave.pcm dmixer
	path /usr/lib/ladspa
	plugins [
	{
		id 4221
		label bs2b
		input { controls [ 700 4.5 ] }
	}
	]
}
#-*- JACK -*-
ctl.jack { type plug; slave.ctl default }
pcm.jack { type plug; slave.pcm jackplug }
pcm.jackplug {
	type jack
	playback_ports 
	{
		0 alsa_pcm:playback_1
		1 alsa_pcm:playback_2
	}
	capture_ports 
	{
		0 alsa_pcm:capture_1
		1 alsa_pcm:capture_2
	}
}
#-*- MBEQ -*-
ctl.eq { type plug; slave.ctl default }
pcm.eq { type plug; slave.pcm plugeq }
pcm.plugeq {
	type ladspa
	slave.pcm dmixer
	path /usr/lib/ladspa
	plugins [
	{
		label mbeq
		id 1197
		input {
			#bands: 50hz,100hz,156hz,220hz,311hz,440hz,622hz,880hz,
			#1250hz,1750hz,25000hz,50000hz,10000hz,20000hz
			controls [ -5 -5 -5 -5 -5 -10 -20 -15 -10 -10 -10 -10 -10 -3 -2 ]
		}
	}
	]
}
#-*- DMIX -*-
ctl.dmixer { type plug; slave.ctl default }
pcm.dmixer { type plug; slave.pcm dmixplug }
pcm.dmixplug {
	type dmix
	ipc_key 1048
	slave {
		pcm pch
		period_time 0
		period_size 1024
		buffer_size 4096
		rate 44100
	}
	bindings {
		0 0
		1 1
	}
}
#-*- EQUALIZER -*-
ctl.equal { type equal }
pcm.equal { type plug; slave.pcm plugequal }
pcm.plugequal { type equal; slave.pcm dmixer }
#-*- OSS ALSA-EMU -*-
#pcm.alsa {
#	type oss
#	device /dev/oss/oss_cmi878x0/pcm1
#}
#mixer.alsa {
#	type oss
#	device /dev/oss/oss_cmi878x0/mix1
#}
#-*- ALSA OSS-EMU -*-
ctl.oss { type oss }
pcm.oss { type plug; slave.pcm dmixer }
